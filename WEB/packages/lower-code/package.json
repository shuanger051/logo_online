    "@tinymce/tinymce-vue": "3.2.8",
    "animate.css": "^4.1.0",
    "ant-design-vue": "1.7.2",
    "core-js": "^3.6.5",
    "lodash": "4.17.21",
    "element-ui": "^2.13.0",
    "font-awesome": "4.7.0",
    "hotkeys-js": "^3.7.6",
